##################################################
##################################################

[Host]
##【IPv6】
# IPv6 Localhost
ip6-localhost = ::1
# IPv6 Loopback
ip6-loopback = ::1
# IPv6 Link-Local
ip6-localnet = fe00::0
# IPv6 Multicast
ip6-mcastprefix = ff00::0
# IPv6 All Nodes
ip6-allnodes = ff02::1
# IPv6 All Routers
ip6-allrouters = ff02::2
# IPv6 All Hosts
ip6-allhosts = ff02::3

